    "build-storybook": "build-storybook --no-dll"
  },
  "dependencies": {
    "autoprefixer": "9.0.0",
    "chart.js": "^2.9.4",
    "core-js": "^3.8.1",
    "package.json": "^2.0.1",
    "postcss": "^8.2.2",
    "postcss-loader": "^4.1.0",
    "tailwindcss": "^1.9.6",
    "vue": "^2.6.11"
  },
  "devDependencies": {
